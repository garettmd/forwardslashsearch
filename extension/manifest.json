{
    "name": "forward slash search",
    "version": "2016.0.1",
    "icons": {
        "128": "/img/icons/icon_128.png"
    },

    "description": "Press / (slash) to focus first search or input field on a page to perform a search.",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "run_at": "document_start",
            "js": [
                    "/js/content_script.js"
            ]
        }
    ],

    "manifest_version": 2
}
